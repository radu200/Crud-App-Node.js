<div class="row">
  <!--  edit admin-sidebar-user-basic in partial folder-->
  {{>dashboard-sidebar}}
  <!-- Main Content -->
  <div class="container-fluid">
    <div class="side-body">
      <div class="col-lg-10">
        <h1>Add Product</h1>
        <hr> {{#if error_msg}} {{#each error_msg}}
        <div class="alert alert-danger">
          {{msg}}
        </div>
        {{/each}} {{/if}} {{#if errors}} {{#each errors}}
        <div class="alert alert-danger">
          {{msg}}
        </div>
        {{/each}} {{/if}} {{#if success_msg}} {{#each success_msg}}
        <div class="alert alert-success">
          {{msg}}
        </div>
        {{/each}} {{/if}}
        <div class="panel panel-default">

          <div class="panel-heading">
            <h3 class="panel-title">Add Product</h3>
          </div>
         

            <div class="panel-body">
              <ul class="nav nav-tabs" role="tablist">
                <a href="/product/list" class="btn btn-default pull-right " type="submit" name="submit" data-toggle="tooltip" data-placement="top"
                  title="Cancel">
                  <i class="fa fa-share" style="color:black"></i>
                </a>
                <button class="btn btn-primary pull-right" type="submit" name="submit" data-toggle="tooltip" data-placement="top" title="Save">
                  <i class="fa fa-save"></i>
                </button>
              </ul>
              <div class="tab-content">
                <div class="tab-pane active" id="general" role="tabpanel">
                  <br>
              

           

            </div>
        </div>
      </div>
    </div>
  </div>
  <div>
  </div>
</div>